.tall {
  width: 94%;
  height: 800rpx;
  border: 1px solid rgb(155, 155, 155);
  margin: 30rpx auto;
}

.picker-view {
  width: 100%;
  display: flex;
  z-index:12;
  background-color: #fff;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  position: fixed;
  bottom: 0rpx;
  left: 0rpx;
  height: 40vh;
}

.picker-item {
  line-height: 70rpx;
  margin-left: 5rpx;
  margin-right: 5rpx;
  text-align: center;
}
.tal {
  width: 90%;
  height: 300rpx;
  margin:500rpx auto;
}
.deta {
  width: 90%;
  margin: 20rpx auto;
  height: 200rpx;
  background: rgba(219, 219, 219, 0.4);
}

.btn {
  width: 240rpx;
  height: 80rpx;
  background: rgba(65, 65, 65, 0.4);
  color: red;
  font-size: 1.1em;
  text-align: center;
  line-height: 80rpx;
}